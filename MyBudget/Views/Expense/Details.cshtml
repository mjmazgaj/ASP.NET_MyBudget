@model MyBudget.Models.Expense

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div style="word-wrap:break-word;">
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NameExp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NameExp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ValueExp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ValueExp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateExp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateExp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DescriptionExp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DescriptionExp)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edytuj", "Edit", new { id = Model.IdExp}, new { @class = "btn btn-outline-secondary" })
    <a asp-action="Index" class="btn btn-outline-secondary">Wróć do listy zadań</a>
</div>
